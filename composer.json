{
  "name": "oliup/oliup-cs-php",
  "version": "1.0.2",
  "description": "Oliup PHP code style rules.",
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": ">=3.11.2",
    "friendsofphp/php-cs-fixer": ">=3.65.0",
    "phpcompatibility/php-compatibility": ">=9.3.5"
  },
  "autoload": {
    "psr-4": {
      "OLIUP\\CS\\": "src"
    }
  },
  "bin": [
    "bin/oliup-cs"
  ],
  "prefer-stable": true
}
