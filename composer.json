{
    "name": "oliup/php-cs-oliup",
    "version": "1.0.0",
    "description": "Oliup PHP code style rules.",
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpcompatibility/php-compatibility": "*"
    },
    "autoload": {
        "psr-4": {
            "OLIUP\\PhpCS\\": "src"
        }
    },
    "bin": [
        "bin/php-cs-oliup"
    ],
    "prefer-stable": true
}
